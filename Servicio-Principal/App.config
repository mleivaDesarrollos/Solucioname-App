<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
      <services>
        <service name="Servicio_Principal.Servicio" behaviorConfiguration="HostBehavior">
          <host>
            <baseAddresses>
              <add baseAddress="http://localhost:8001/Servicio-Principal"/>
              <add baseAddress="net.tcp://localhost:8000/Servicio-Principal"/>
            </baseAddresses>
          </host>
          <endpoint name="DualTCPBinding" address="tcp" binding="netTcpBinding" bindingConfiguration="DualTCPBinding" contract="Servicio_Principal.IServicio">
            <identity>
              <dns value="localhost"/>
            </identity>          
          </endpoint>
          <endpoint name="MetadataHTTPEndpoint" address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        </service>
      </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="HostBehavior">
            <serviceDebug includeExceptionDetailInFaults="true"/>
            <serviceMetadata httpGetEnabled="true"/>
            <serviceThrottling maxConcurrentCalls="100" maxConcurrentSessions="100"/>
          </behavior>
        </serviceBehaviors>
      </behaviors>
      <bindings>
        <netTcpBinding>
          <binding name="DualTCPBinding" closeTimeout="00:01:00" openTimeout="00:00:10" sendTimeout="00:01:00" receiveTimeout="00:00:01" maxConnections="100">
            <reliableSession enabled="true" inactivityTimeout="00:20:00"/>
            <security mode="None"></security>            
          </binding>
        </netTcpBinding>
      </bindings>
    </system.serviceModel>
  <connectionStrings>
    <add name="CadenaServicio" connectionString="Data Source=C:\Users\B034466\Documents\Visual Studio 2015\Projects\SolSolucioname\SolucionameServicio.db;Version=3;"/>
  </connectionStrings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/></startup></configuration>
